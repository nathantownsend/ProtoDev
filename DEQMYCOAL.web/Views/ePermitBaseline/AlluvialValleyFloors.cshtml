            <button class="tertiary"><span class="plussymbol">+</span>&nbsp;Add New</button>
        </div>
    </fieldset>

    @*alluvial valley floor map layers*@

    <fieldset class="setapart">
        <legend>Alluvial Valley Floor Maps</legend>
        <h4>Required Map Layers:</h4>
        <div class="checkboxes">
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Farms and Ranches (17.24.325(3)(d)(v))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Flood Irrigated Lands (17.24.325(2)(a)(ii))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Watersheds (17.24.325(2)(a)(i))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Cultural Resource Sites (17.24.304(1)(b))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Streamlaid Deposits (17.24.304(1)(b))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Streams (17.24.304(1)(b))</a>
            <br />
