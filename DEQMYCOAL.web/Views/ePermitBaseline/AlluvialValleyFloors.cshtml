            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Premine Topography (17.24.304(1)(b))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Landuse (17.24.304(1)(b))</a>
            <br />
            <input disabled="disabled" type="checkbox" name="LandUse" value="x" checked="checked" /><a href="@Url.Action("Maps", "ePermitMaps")">Historic Flood Irrigation Areas (17.24.304(1)(b))</a>
